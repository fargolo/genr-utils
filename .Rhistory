h1wordtab <- function (data,var.num,var.cat,var.group, dec.plat= 1,dec.p = 3){
#Defines cathegorical variables row length: 1 line for each cathegory + 1 line for variable name
len.cat <- length(summary(data[var.cat])) + length(var.cat)
#Defines row length: 1 for each numerical var + catheg var row len + 1 (Heading)
row.len <- length(var.num) + len.cat + 1
#Defines col lenght: one for each group + 1 for total + 1 for Variable names + 1 for p values
col.len <- length(summary(data[var.group])) + 3
#Defines lenght of final table
final.tab <- matrix (nrow = row.len, ncol = col.len)
final.tab[1,1] <- c("Variables")
final.tab[1,2] <- paste("Total(n =",nrow(data),")")
cur.col <- 3
final.tab[1,cur.col] <- paste(")")
return(final.tab)
}
